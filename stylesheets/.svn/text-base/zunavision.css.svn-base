/* Divisions */

div#main {
	width: 800px;
	height: auto;
	padding-top: 0px;
	padding-right: 0px;
	padding-bottom: 5px;
	padding-left: 0px;
	background-color: #FFFFFF;
	margin:0 auto;
	
}

div#content {
	width: 770px;
	height: auto;
	padding-top: 0px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
	background-color: #FFFFFF;
	margin:0 auto;
	font-family: Tahoma, Verdana;
	font-size: 79%;
}

body {
	background-color: #E3ECF0;/*#D9E4EA;*/
}


div#border_bottom {
margin:0 auto;
width: 800px;
padding: 20px 0 0px 0;
overflow: hidden;
background: url("../images/border_bottom.gif") no-repeat;
height: 0px !important; /* for most browsers */
height /**/:20px; /* for IE5.5's bad box model */
}

div#logo {
margin:0 auto;
width: 800px;
padding: 0px 0 0px 0;
overflow: hidden;
