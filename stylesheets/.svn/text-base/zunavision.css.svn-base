/* Formatting Tags */

a#attention
{

   font:tahoma, verdana, sans-serif;
	
font-size: 20px;
    color: #557f96;
   /*font-weight: bold;*/
}

h2{
	font-size: 13pt;
	font-weight: 400;
	margin: 0 0 12px;
	color: #557f96;
}

#smalltext{
	font-size: 10pt;
	color: #999999;
}

#smalltext a{
	font-size: 10pt;
	color: #999999;
}

a:link , a:visited
{
	color: #557f96;
	font-size: 11pt;
	text-decoration: none;
}

a img { border: 0px; }

a:hover
{
	color: #557f96;
	font-size: 11pt;
	text-decoration: underline;
}


/* End of Formatting Tags*/

/*Tablist*/

#tablist{
list-style:none;
padding: 0;
margin:0 auto;
}

#tablist li {
float: left;
display: block;
margin:0;
}

#tablist a , #othertabs a{
display: block;
float: left;
font-family: tahoma;
font-size: 11pt;
clear: both;
text-decoration:none;
}


#othertabs a{
display: block;
float: left;
font-family: tahoma;
font-size: 13pt;
clear: both;
text-decoration:none;
}


#othertabs{
list-style:none;
padding: 0;
margin:0 auto;
margin-top: 10px;
float: right;

}

